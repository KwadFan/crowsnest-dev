# shellcheck enable=require-variable-braces

# Exit upon Errors
set -Eex


BASE_CN_PATH="$(dirname "$(readlink -f "${0}")")"

import_components() {
    local components
    components=""
}

main() {

    echo "${BASE_CN_PATH}"


    import_components

}

if [[ "${BASH_SOURCE[0]}" = "${0}" ]]; then
    main
fi
